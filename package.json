{
  "name": "svelte-avatar",
  "homepage": "https://svelte-avatar.netlify.app/",
  "keywords": [
    "svelte"
  ],
  "version": "0.1.4",
  "description": "An avatar component for Svelte",
  "repository": "git@github.com:wdtamagi/svelte-avatar.git",
  "author": "Willian Dias Tamagi",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "src/lib/Avatar.svelte",
    "README.md"
  ],
  "svelte": "src/lib/Avatar.svelte",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest"
  },
  "dependencies": {
    "initials": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "rollup": "^2.12.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^6.1.0",
    "sirv-cli": "^0.4.6",
    "svelte": "^3.23.0",
    "svelte-highlight": "^0.6.2",
    "svelte-jester": "^1.0.6"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": "svelte-jester",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
